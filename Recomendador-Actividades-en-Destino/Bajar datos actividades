actividades <- rbind(act,tra,ins)


act.idcro.jul <- subset(actividades, is.na(idcro)==F & idcro!="" )
act.idcro.jul <- subset(act.idcro.jul,nchar(act.idcro.jul$idcro) == 8)
act.idcro.jul <- as.data.table(act.idcro.jul)
act.idcro.jul <- act.idcro.jul[!duplicated(act.idcro.jul[,idcro]),]

save(act.idcro.jul, file="idcrojulio.rda")

act.idcro <- rbind (act.idcro.jul,act.idcro.ago, act.idcro.sep)
